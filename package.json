{
  "name": "react-with-observable",
  "version": "2.0.1",
  "description": "Use Observables with React declaratively!",
  "main": "build/index.common.js",
  "browser": "build/index.js",
  "unpkg": "build/index.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:mmiszy/react-with-observable.git"
  },
  "bugs": {
    "url": "https://github.com/mmiszy/react-with-observable/issues"
  },
  "homepage": "https://github.com/mmiszy/react-with-observable#readme",
  "scripts": {
    "build": "rimraf build && rollup -c",
    "test": "npm-run-all -p test:jest test:types",
    "test:cypress": "cypress run --record",
    "test:e2e": "npm-run-all build:examples -p --race serve:examples test:cypress",
    "test:jest": "jest --detectLeaks",
    "test:types": "tslint -c ./tslint.json --project ./tsconfig.json",
    "test:coverage": "jest --detectLeaks --coverage",
    "test:coveralls": "npm run test:coverage && cat coverage/lcov.info | coveralls ",
    "build:examples": "webpack --config examples/webpack.config.js --mode production",
    "serve:examples": "http-server ./examples -a localhost -p 8082 -c-1",
    "start:examples": "webpack-dev-server --config examples/webpack.config.js",
    "deploy:examples": "npm run build:examples && gh-pages --dist examples",
    "prettier": "prettier --config .prettierrc --write '**/*.{ts,js,tsx,jsx}'",
    "release": "np --no-yarn",
    "prepublish": "npm run build",
    "postdeploy": "npm run deploy:examples"
  },
  "keywords": [],
  "author": "Micha≈Ç Miszczyszyn <michal@mmiszy.pl> (https://typeofweb.com/)",
  "license": "ISC",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "2.0.1",
    "@types/create-subscription": "16.4.0",
    "@types/enzyme": "3.1.11",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/jest": "23.1.3",
    "@types/react-dom": "16.0.6",
    "coveralls": "3.0.2",
    "create-subscription": "16.4.1",
    "cypress": "3.0.2",
    "danger": "3.7.19",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "gh-pages": "1.2.0",
    "git-revision-webpack-plugin": "3.0.3",
    "http-server": "0.11.1",
    "husky": "1.0.0-rc.9",
    "jest": "23.2.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.7",
    "pretty-quick": "1.6.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "rimraf": "2.6.2",
    "rollup": "0.62.0",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-typescript2": "0.18.1",
    "rxjs": "6.2.1",
    "source-map-support": "0.5.6",
    "symbol-observable": "1.2.0",
    "ts-jest": "23.0.0",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "typestrict": "0.0.9",
    "weak": "1.0.1",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "peerDependencies": {
    "react": ">=16.3.0 <17.0.0",
    "react-dom": ">=16.3.0 <17.0.0",
    "create-subscription": ">=16.3.0 <17.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test"
    }
  }
}
